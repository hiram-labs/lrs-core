  "scripts": {
    "dev-api-server": "babel-node api/build",
    "dev-ui-server": "babel-node ui/buildServer",
    "dev-ui-client": "babel-node ui/buildClient",
    "dev-cli-server": "babel-node cli/build",
    "dev-worker-server": "babel-node worker/build",
    "dev-web-services": "babel-node api/build && babel-node ui/buildServer && babel-node ui/buildClient",
    "build-api-server": "babel-node api/build --release",
    "build-ui-server": "babel-node ui/buildServer --release",
    "build-ui-client": "babel-node ui/buildClient --release",
    "build-cli-server": "babel-node cli/build --release",
    "build-worker-server": "babel-node worker/build --release",
    "build-web-services": "babel-node api/build --release && babel-node ui/buildServer --release && babel-node ui/buildClient --release",
    "build-all": "babel-node api/build --release && babel-node ui/buildServer --release && babel-node ui/buildClient --release && babel-node cli/build --release && babel-node worker/build --release",
    "start-api-server": "node api/dist/server",
    "start-ui-server": "node ui/dist/server",
    "start-worker-server": "node worker/dist/server",
    "start": "pm2 start pm2/all.json",
    "start-dev": "pm2 start pm2/dev-all.json",
    "restart": "pm2 restart pm2/all.json",
    "restart-dev": "pm2 restart pm2/dev-all.json",
    "restart-test": "pm2 restart pm2/test-all.json",
    "stop": "pm2 delete pm2/all.json",
    "stop-dev": "pm2 delete pm2/dev-all.json",
    "stop-test": "pm2 delete pm2/test-all.json",
    "lint": "eslint -c .eslintrc ui/src api/src cli/src worker/src lib",
    "test-api": "mocha-webpack --bail --exit --webpack-config \"api/testConfig.js\" \"api/src/**/*-test.js\"",
    "test-worker": "mocha-webpack --bail --exit --webpack-config \"worker/testConfig.js\" \"worker/src/**/*-test.js\"",
    "test-lib": "mocha-webpack --bail --exit --webpack-config \"api/testConfig.js\" \"lib/**/*-test.js\"",
    "test-cli": "mocha-webpack --bail --exit --webpack-config \"cli/testConfig.js\" \"cli/**/*-test.js\" --include lib/models",
    "test-ui": "jest --bail --config jest.config.json --env node",
    "test": "npm run test-api && npm run test-lib && npm run test-worker && npm run test-ui && npm run test-cli",
    "migrate": "node cli/dist/server migrateMongo --up",
    "rollback": "node cli/dist/server migrateMongo --down last",
    "clean": "rimraf cli/dist worker/dist api/dist ui/dist cli/src/node_modules worker/src/node_modules api/src/node_modules ui/src/node_modules",
    "fmt": "prettier './**/*.{json,js}' --write"
  },
